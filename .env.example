# Debug Configuration
# Set to 'true' to enable detailed logging of service responses
DEBUG=false

# Server Configuration
PORT=3000
HOST=0.0.0.0
NODE_ENV=development
API_DOMAIN=http://localhost:3000

# Authentication
API_TOKEN=your-secure-api-token-here

# Geocoding Services Configuration
# Comma-separated list of services to use. Supported: google-maps, geocodio
GEO_SERVICES=google-maps

# Google Maps API (required if google-maps is in GEO_SERVICES)
GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Geocodio API (required if geocodio is in GEO_SERVICES)
GEOCODIO_API_KEY=your-geocodio-api-key

# Service Configuration
ADDRESS_SERVICE_TIMEOUT=5000

# Cache Configuration
CACHE_MAX_SIZE=1000
CACHE_TTL_MS=3600000

